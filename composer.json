{
    "name": "sinergi/devtools",
    "type": "library",
    "description": "Set of PHP development tools to allow working on multiple projects",
    "keywords": ["Development", "Tools", "Dependency", "Workflow"],
    "homepage": "https://github.com/sinergi/devtools",
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Bull",
            "email": "me@gabrielbull.com"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4",
        "composer/composer": "~1@dev"
    },
    "autoload": {
        "psr-4": {
            "Sinergi\\Devtools\\": "src/Devtools"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sinergi\\Devtools\\Tests\\": ["tests/Devtools/Tests", "tests/Devtools/Tests/_includes"]
        }
    },
    "bin": [
        "bin/composerupdateall"
    ]
}
